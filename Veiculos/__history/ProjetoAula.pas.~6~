  Form1: TForm1;

implementation

{$R *.dfm}

procedure TForm1.btnInserirClick(Sender: TObject);
begin

  if (Edit7.text = '') then
  begin
    MessageDlg('O campo Nome não pode estar em branco', mtError, [mbOK], 0);
    Edit7.SetFocus;
    exit;
  end
  else if (Edit1.text = '') then
  begin
    MessageDlg('O campo Placa não pode estar em branco', mtError, [mbOK], 0);
    Edit1.SetFocus;
    exit;
  end
  else
  begin
    MessageDlg('Veículo cadastrado com sucesso!', mtInformation, [mbOK],0);
  end;
end;

end.
